snippet lis
    int LIS(const vi & v){
        int n = sz(v), ans = 0;
        vi u(n, INF);
        for(auto x: v){
            // auto itr = upper_bound(u.begin(), u.begin() + ans + 1, x);  // <=
            auto itr = lower_bound(u.begin(), u.begin() + ans + 1, x); // <
            ans += *itr==INF ? 1 : 0;
            *itr = x;
        }
        return ans;
    }

