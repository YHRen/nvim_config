snippet ispal
    bool is_palin(const string & s){
        int n = sz(s);
        forall(i,0,n/2) if(s[i] != s[n-i-1]) return false;
        return true;
    }

# manacher returns largest palindrome radius centered at i.
snippet manacher
        vector<int> manacher(string & s){
        vector<int> v(s.size());
        int n = int(s.size());
        int i = 0, j = 0, k = 1;
        while( i < n ){
            while( i-j >= 0 && i+j < n && s[i-j] == s[i+j] ) ++j;
            v[i] = j;
            k = 1;
            while( i-k >= 0 && k+v[i-k] < j ) v[i+k] = v[i-k], ++k;
            i += k, j -= k;
        }
        return std::move(v);
    }

snippet unionfind
    struct UF {
        vector<int> e;
        UF(int n) : e(n, -1) {}
        bool same_set(int a, int b) { return find(a) == find(b); }
        int size(int x) { return -e[find(x)]; }
        int find(int x) { return e[x] < 0 ? x : e[x] = find(e[x]); }
        void join(int a, int b) {
            a = find(a), b = find(b);
            if (a == b) return;
            if (e[a] > e[b]) swap(a, b);
            e[a] += e[b]; e[b] = a;
        }
    };

# fenwick supports log(n) point update and range query
snippet fenwick
    struct Fenwick { // one-based binary indexed tree
        vector<int> bit;
        int n;

        Fenwick(int n) {
            this->n = n + 1;
            bit.assign(n + 1, 0);
        }

        Fenwick(vector<int> a) : Fenwick(a.size()) {
            for (size_t i = 0; i < a.size(); i++)
                add(i, a[i]);
        }

        int sum(int idx) {
            // inclusive [0, idx]
            int ret = 0;
            for (++idx; idx > 0; idx -= idx & -idx)
                ret += bit[idx];
            return ret;
        }

        int sum(int l, int r) {
            // inclusive [l, r]
            return sum(r) - sum(l - 1);
        }

        void add(int idx, int delta) {
            for (++idx; idx < n; idx += idx & -idx)
                bit[idx] += delta;
        }

        /*
            range update and point query mode:
            range_add [l, r], x: add(l, x); add(r+1, -x);
            point_query idx: sum(idx);
        */
    };

snippet fenwick2
    template <typename T>
    struct Fenwick2D {
    vector<vector<T>> tree;
    const T initVal;
    function<T (T, T)> op;

    // constructor
    // n, m : dimension of elems, init : initial value
    // op : associative & commutative operator
    // O(n*m)
    Fenwick2D(int n, int m, T init, function<T (T, T)> _op)
        : tree(n + 1, vector<T>(m + 1, init)), initVal(init), op(_op) {
    }

    // fill initial value in all nodes
    // O(n*m)
    void clear() {
        for (auto& v : tree) fill(v.begin(), v.end(), initVal);
    }

    // get value of  a[1][1..y] * a[2][1..y] * ... * a[x][1..y]
    // 1 <= x <= n  ,  1 <= y <= m
    // O(log(n)*log(m))
    T query(int x, int y) {
        T ret = initVal;
        for (; x > 0; x -= x & -x) {
        for (int ty = y; ty > 0; ty -= ty & -ty)
            ret = op(ret, tree[x][ty]);
        }
        return ret;
    }

    // apply val to (x, y)
    // 1 <= x <= n  ,  1 <= y <= m
    // O(log(n)*log(m))
    void update(int x, int y, T val) {
        for (; x < tree.size(); x += x & -x) {
        for (int ty = y; ty < tree[x].size(); ty += ty & -ty)
            tree[x][ty] = op(tree[x][ty], val);
        }
    }
    };

snippet eulerian_path
alias hierholzer
    vvi eulerian_path(vvi& edges) {
        // lc2097
        unordered_map<int, vi> G;
        unordered_map<int, int> cnt;
        for(auto & p : edges){
            G[p[0]].pb(p[1]);
            cnt[p[0]] ++;
            cnt[p[1]] --;
        }
        auto itr = find_if(all(cnt), [](auto & p){ return p.second > 0; });
        if(itr == cnt.end()) itr = cnt.begin();
        stack<int> S;
        S.push(itr->first);
        vi R;
        while(!S.empty()){
            for(int x = S.top(), y; in(x, G) && !G[x].empty(); x = y){
                y = G[x].back();
                G[x].pop_back();
                S.push(y);
            }
            R.pb(S.top());
            S.pop();
        }
        vvi ans;
        for(int i = sz(R)-1; i >= 1; --i){
            ans.pb({R[i], R[i-1]});
        }
        return ans;
    }

snippet reverselist
    ListNode* reverse(ListNode * head){
        ListNode * prv, * cur, * nxt;
        cur = head;
        while(cur!=nullptr){
            nxt = cur->next;
            cur->next = prv;
            prv = cur;
            cur = nxt;
        }
        return prv;
    }

snippet hungarian
alias bipartite
    pair<int, vi> hungarian(const vvi &G) {
        // G is NxM adjacent matrix
        // Negate costs for max cost.
        if (G.empty()) return {0, {}};
        int n = sz(G) + 1, m = sz(G[0]) + 1;
        vi u(n), v(m), p(m), ans(n - 1);
        forall(i,1,n){
            p[0] = i;
            int j0 = 0; // add "dummy" worker 0
            vi dist(m, INF), pre(m, -1);
            vector<bool> done(m + 1);
            do { // dijkstra
                done[j0] = true;
                int i0 = p[j0], j1;
                auto delta = INF;
                forall(j,1,m) if (!done[j]) {
                    auto cur = G[i0 - 1][j - 1] - u[i0] - v[j];
                    if (cur < dist[j]) dist[j] = cur, pre[j] = j0;
                    if (dist[j] < delta) delta = dist[j], j1 = j;
                }
                forall(j,0,m) {
                    if (done[j]) u[p[j]] += delta, v[j] -= delta;
                    else dist[j] -= delta;
                }
                j0 = j1;
            } while (p[j0]);
            while (j0) { // update alternating path
                int j1 = pre[j0];
                p[j0] = p[j1], j0 = j1;
            }
        }
        forall(j,1,m) if (p[j]) ans[p[j] - 1] = j - 1;
        return {-v[0], ans}; // min cost
    }


snippet trie
    struct Trie {
      static constexpr int c_sz = 26;
      bool is_leaf;
      Trie *children[c_sz];
    
    public:
      Trie() {
        this->is_leaf = false;
        forall(i, 0, c_sz) { this->children[i] = nullptr; }
      }
      void insert(const string &s) {
        Trie *curr = this;
        for (auto c : s) {
          int idx = c - 'a';
          if (curr->children[idx] == nullptr) {
            curr->children[idx] = new Trie();
          }
          curr = curr->children[idx];
        }
        curr->is_leaf = true;
      }
    
      bool search(const string &s) {
        Trie *curr = this;
        for (auto c : s) {
          int idx = c - 'a';
          curr = curr->children[idx];
          if (curr == nullptr) {
            return false;
          }
        }
        return this->is_leaf;
      }
    };
